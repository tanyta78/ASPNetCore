@model ChushkaWebApp.Data.Models.Order[]

<h1 class="text-center">All Orders</h1>
<hr class="bg-secondary w-50" />

<table class="table table-striped table-hover">
    <thead>
        <tr>
            <th>#</th>
            <th>Order ID</th>
            <th>Customer</th>
            <th>Product</th>
